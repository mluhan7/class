.disable {
  user-select: none;
  color: #e7e4e4;
}

.container {
  padding: 0 32rpx 10px 32rpx;
  /*background-color: pink;
  background-image: url(images/background.png);
  background-size: cover;
  background-attachment:fixed;*/
}
.date {
  font-size: 12px;
  height: 16px;
}
.header {
  margin: 5px 0 15rpx 0;
  padding-bottom: 10px;
  font-size: 14px;
  border-bottom: 1px solid #c5c3c3;
}
.header>view+view {
  margin-top: 8rpx;
}
.pushright {
  width: 60px;
  color: gray;
}
.bt-header {
  font-size: 14px;
  margin-right: 20rpx;
  padding: 0 8px;
  height: 24px;
  line-height: 24px;
  /*background-color: #e7e4e4;*/
}
.bt-access {
  font-size: 14px;
  color: white;
  font-weight: 700;
  margin-top: 8px;
  background-color: #333333;
}

/*课程表*/
.swaper-tt {
  font-size: 14px;
}
.left {
  display: inline-block;
  width: 28px;
  margin-right: 3px;
}
.title {
  height: 20px;
  margin-bottom: 5px;
  font-size: 14px;
  color: gray;
}
.room {
  height: 26px;
  line-height: 26px;
  border: 1px solid #333333;
  border-radius: 5px;
  margin-bottom: 2px;
  font-size: 12px;

  text-align: center;
}
.right {
  display: inline-block;
  flex-grow: 1;
}
/*1~2节*/
.t-item {
  display: inline-block;
  box-sizing: border-box;
  width: 20%;
  text-align: center;
}

.line {
  width: 100%;
}
.c-common {
  box-sizing: border-box;
  width: 20%;
  height: 28px;
  border-radius: 5px;
  margin-bottom: 2px;
  margin-left: 2px;
  border: 1px solid transparent;
  background-color: #e7e4e4;
  color: transparent;

}
/*.c-common>.c-common+.c-common
{
  margin-left: 2px;
}*/

.free0 {
  background-color: #00c777;
  text-align: center;
  line-height: 28px;
  color: white;
}


/*签名模块*/
.sign-container{
  margin-top: 40rpx;
  margin-bottom: 8px;
  justify-content: center;  
  width: 100%;
}
.my-sign{
  width: 100%;
  border-bottom: 1px solid #333;
}
.my-sign input{
  width: 100%;
}
.sign-placeholder{
  font-size: 14px;
}
.sign-image{
  width: 45rpx;
  height: 45rpx;
}

/*便签*/
.note-container{
  display: flex;
  align-items: center;
  justify-content: center;  
  width: 100%;
  font-size: 16px;
}
.my-note{
  border: 1px solid #333;
  border-radius: 4px;
  padding: 8px;
  min-height: 20px;
  width: 100%;
}
.note-placeholder{
  font-size: 14px;
}