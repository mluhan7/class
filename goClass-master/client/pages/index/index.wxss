/* pages/index/index.wxss */
.wrapper {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 0 80px;
}

.content {
  font-size: 15px;
  text-align: center;
}
.wrap-logo {
  position: relative;
}
.logo {
  height: 184rpx;
  width: 184rpx;
  position: relative;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.t-add {
  font: 13px;
}

.bottom {
    border-radius: 80rpx;
    font-size: 15px;
    margin-top: 25px;
}
